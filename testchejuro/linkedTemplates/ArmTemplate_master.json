{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "testchejuro"
		},
		"AzureDataLakeStorage 2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://testchejuro1.dfs.core.windows.net/"
		},
		"CosmosDbNoSql1_properties_typeProperties_accountEndpoint": {
			"type": "string",
			"defaultValue": "https://testchejuro.documents.azure.com:443/"
		},
		"CosmosDbNoSql1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "test"
		},
		"AzureBlobStorage2_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://testchejuro1.blob.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "testchejuro_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage 2_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage 2_properties_typeProperties_url')]"
					},
					"CosmosDbNoSql1_properties_typeProperties_accountEndpoint": {
						"value": "[parameters('CosmosDbNoSql1_properties_typeProperties_accountEndpoint')]"
					},
					"CosmosDbNoSql1_properties_typeProperties_database": {
						"value": "[parameters('CosmosDbNoSql1_properties_typeProperties_database')]"
					},
					"AzureBlobStorage2_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('AzureBlobStorage2_properties_typeProperties_serviceEndpoint')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}